<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.moyo.Beans.SurevyEntity" table="Surevy" schema="dbo" catalog="SurveyWeb">
        <id name="naireId">
            <column name="naireId" sql-type="varchar(10)" length="10"/>
        </id>
        <property name="naireName">
            <column name="naireName" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="description">
            <column name="description" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="createTime">
            <column name="createTime" sql-type="date" not-null="true"/>
        </property>
        <property name="batchId">
            <column name="batchId" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>