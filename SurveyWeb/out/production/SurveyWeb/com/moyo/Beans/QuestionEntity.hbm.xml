<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.moyo.Beans.QuestionEntity" table="Question" schema="dbo" catalog="SurveyWeb">
        <id name="questionId">
            <column name="questionId" sql-type="varchar(10)" length="10"/>
        </id>
        <property name="type">
            <column name="type" sql-type="int" not-null="true"/>
        </property>
        <property name="order">
            <column name="order" sql-type="int" not-null="true"/>
        </property>
        <property name="content">
            <column name="content" sql-type="varchar(500)" length="500" not-null="true"/>
        </property>
        <property name="naireId">
            <column name="naireId" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>